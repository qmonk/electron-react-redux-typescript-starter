{
    "name": "electron-react-redux-typescript-starter",
    "version": "0.0.0",
    "main": "./dist/main.js",
    "scripts": {
        "dev-main": "cross-env NODE_ENV=development nodemon",
        "dev-renderer": "cross-env NODE_ENV=development webpack serve --config webpack.renderer.config.js",
        "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
        "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
        "build": "npm run build-main && npm run build-renderer",
        "pack": "npm run build && electron-builder --dir",
        "dist": "npm run build && electron-builder",
        "lint": "eslint --ext=jsx,js,tsx,ts ./src",
        "test": "jest ./test",
        "test-watch": "npm run test -- --watch"
    },
    "build": {
        "directories": {
            "output": "release"
        },
        "files": [
            "dist/",
            "node_modules/",
            "package.json"
        ],
        "linux": {
            "target": "deb"
        },
        "win": {
            "target": "nsis"
        }
    },
    "author": {
        "name": "test",
        "email": "test@test.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/qmonk/electron-react-redux-typescript-starter"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@types/react-redux": "^7.1.20",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-loader": "^8.2.3",
        "cross-env": "^7.0.3",
        "electron": "^13.6.1",
        "electron-builder": "^22.14.5",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fork-ts-checker-webpack-plugin": "^6.4.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.14",
        "prettier": "^2.4.1",
        "react-redux": "^7.2.6",
        "redux": "^4.1.2",
        "typescript": "4.4.4",
        "webpack": "^5.61.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.4.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@babel/plugin-transform-runtime": "^7.16.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    }
}
